{% extends "base" %}
{% block content %}
<div
  class="w-50 h-100 top-0 start-50
  position-absolute translate-middle-x
  bg-secondary">
  <div class="list-group list-group-flush">
    {% for tweet in tweets %}
    <a href="/tweet/{{ tweet.id }}"
      class="list-group-item list-group-item-action"
      style="background: #1DA1F2">
      {{ tweet.title }}
    </a>
    {% endfor %}
  </div>
</div>
{% endblock %}
{% block head %}
<title>Rust Tweet</title>
{% endblock %}
